{
  "project": {
    "name": "To-Do List App with AI Features",
    "description": "A to-do list application that leverages AI to enhance user productivity through intelligent task management and automated suggestions.",
    "techStack": [
      "Next.js 14",
      "Supabase",
      "TypeScript",
      "Tailwind CSS",
      "React Query",
      "Zod"
    ]
  },
  "frontendStructure": {
    "pages": {
      "/": {
        "type": "SSR",
        "components": [
          "Header",
          "TaskList",
          "Dashboard",
          "Footer"
        ],
        "authRequired": true,
        "authRedirect": null,
        "dataFetching": [
          "fetchUserTasks",
          "fetchUserRecommendations"
        ]
      },
      "/login": {
        "type": "Static",
        "components": [
          "LoginForm",
          "SocialLogin"
        ],
        "authRequired": false,
        "authRedirect": null,
        "dataFetching": []
      },
      "/register": {
        "type": "Static",
        "components": [
          "RegistrationForm"
        ],
        "authRequired": false,
        "authRedirect": null,
        "dataFetching": []
      },
      "/settings": {
        "type": "SSR",
        "components": [
          "SettingsForm",
          "DarkModeToggle"
        ],
        "authRequired": true,
        "authRedirect": null,
        "dataFetching": [
          "fetchUserSettings"
        ]
      }
    },
    "components": {
      "shared": [
        "Button",
        "Modal",
        "InputField",
        "Notification"
      ],
      "feature": [
        "TaskForm",
        "TaskCard",
        "DashboardCharts",
        "CollaborationInvite"
      ]
    }
  },
  "apiRoutes": {
    "auth": {
      "/api/auth/login": {
        "method": "POST",
        "middleware": [
          "auth checks"
        ],
        "validation": [
          "email",
          "password"
        ],
        "supabase": "supabase.auth.signIn({ email, password })",
        "body": null,
        "response": null,
        "pagination": null
      },
      "/api/auth/register": {
        "method": "POST",
        "middleware": [],
        "validation": [
          "email",
          "password"
        ],
        "supabase": "supabase.auth.signUp({ email, password })",
        "body": null,
        "response": null,
        "pagination": null
      }
    },
    "tasks": {
      "/api/tasks": {
        "method": "POST",
        "middleware": [
          "auth checks"
        ],
        "validation": [
          "title",
          "dueDate",
          "priority"
        ],
        "supabase": "supabase.from('tasks').insert([{ title, dueDate, priority }])",
        "body": null,
        "response": null,
        "pagination": null
      },
      "/api/tasks/:id": {
        "method": "DELETE",
        "middleware": [
          "auth checks"
        ],
        "validation": [],
        "supabase": "supabase.from('tasks').delete().eq('id', id)",
        "body": null,
        "response": null,
        "pagination": null
      }
    },
    "recommendations": {
      "/api/recommendations": {
        "method": "GET",
        "middleware": [
          "auth checks"
        ],
        "validation": [],
        "supabase": "supabase.rpc('get_recommendations', { user_id: user.id })",
        "body": null,
        "response": null,
        "pagination": null
      }
    }
  },
  "supabaseConfig": {
    "tables": {
      "tasks": {
        "columns": {
          "id": "uuid",
          "user_id": "uuid",
          "title": "text",
          "due_date": "timestamp",
          "priority": "integer",
          "completed": "boolean"
        },
        "RLS": {
          "policy": "user_id = auth.uid()"
        }
      },
      "users": {
        "columns": {
          "id": "uuid",
          "email": "text",
          "created_at": "timestamp"
        },
        "RLS": {
          "policy": "id = auth.uid()"
        }
      }
    },
    "indexes": [
      "CREATE INDEX idx_user_tasks ON tasks(user_id)",
      "CREATE INDEX idx_task_priority ON tasks(priority)"
    ]
  },
  "features": {
    "category": [
      "User authentication with email and social login",
      "Task creation with due dates and priority settings",
      "AI-generated task recommendations based on user habits",
      "Natural language processing for task input (e.g., 'remind me to call John tomorrow')",
      "Reminders and notifications for upcoming tasks",
      "Categorization and tagging of tasks for better organization",
      "Collaboration features allowing users to share task lists with others",
      "Dashboard with task visualization (e.g., progress bars, completion rates)",
      "Dark mode and other UI/UX enhancements for better accessibility",
      "Integration with calendar services (e.g., Google Calendar)"
    ]
  },
  "dependencies": {
    "required": [
      "next",
      "react",
      "react-dom",
      "@supabase/supabase-js",
      "tailwindcss",
      "react-query"
    ],
    "dev": [
      "typescript",
      "eslint",
      "prettier",
      "@types/react",
      "@types/node"
    ]
  },
  "environmentVariables": {
    "SUPABASE_URL": "string",
    "SUPABASE_ANON_KEY": "string",
    "NEXTAUTH_SECRET": "string"
  },
  "acceptanceCriteria": [
    "User can register and log in using email and social accounts.",
    "Tasks can be created with due dates and priority settings.",
    "AI recommendations are generated based on task history.",
    "Users can input tasks using natural language.",
    "Users receive reminders and notifications for upcoming tasks.",
    "Tasks can be categorized and tagged.",
    "Users can share task lists with other users.",
    "Dashboard displays task visualization properly.",
    "Dark mode can be toggled by the user.",
    "Integration with external calendars works correctly."
  ]
}